<style>
table {
  font-family: arial, sans-serif;
  border-collapse: collapse;
  width: 100%;
}

td, th {
  border: 1px solid #dddddd;
  text-align: left;
  padding: 8px;
}

</style>

<h1>Requests</h1>
<table>
    
        <tr>
            <th>Start Date</th>
            <th>End Date</th>
            <th>Cat ID</th>
        </tr>
    

    <% @requests.each do |request|%>
    <tr>
        <td><%= request.start_date %> </td>
        <td><%= request.end_date %> </td>
        <td><%= request.cat_id %> </td>
    </tr>
    <% end %>
    
</table>